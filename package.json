{
  "name": "orchestra",
  "version": "1.3.3",
  "description": "Insightpool's official UI kit",
  "repository": "https://github.com/insightpool/orchestra",
  "main": "dist/ui.build.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:all": "npm run build:icons /&& webpack --config webpack.config.js && webpack --config webpack.example.js && webpack --config webpack.test.js && npm run build:docs",
    "build:docs": "./node_modules/.bin/markdown-include markdown.json",
    "build:example": "npm run build:icons && webpack --config webpack.example.js",
    "build:icons": "node src/lib/buildIcons.js && node src/lib/combineIcons.js",
    "build:intern": "node tests/support/intern.js ./tests/functional",
    "build:test-ui": "npm run build:icons && webpack --config webpack.test.js",
    "build:ui": "npm run build:icons && webpack --config webpack.config.js",
    "intern:browserstack": "./node_modules/intern/bin/intern-runner.js config=tests/intern.browserstack.js",
    "intern:local": "./node_modules/intern/bin/intern-runner.js config=tests/intern.local.js",
    "intern:grid": "./node_modules/intern/bin/intern-runner.js config=tests/intern.grid.js",
    "test:browserstack": "npm run build:test-ui && npm run build:intern && npm run intern:browserstack",
    "test:local": "npm run build:test-ui && npm run build:intern && npm run intern:local",
    "test:grid": "npm run build:test-ui && npm run build:intern && npm run intern:grid"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.7",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "classnames": "^2.2.4",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "intern": "^3.4.2",
    "markdown-include": "^0.4.3",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.9.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "sass-loader": "^3.2.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "svgo": "^0.6.6",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "spinkit": "^1.2.5"
  }
}
